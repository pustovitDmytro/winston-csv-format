{
  "name": "winston-csv-format",
  "version": "2.0.3",
  "description": "writes winston logs in csv format",
  "main": "./lib",
  "scripts": {
    "prepare": "babel src --out-dir ./lib --copy-files && husky install",
    "coveralls": "cat ./reports/coverage/lcov.info | coveralls",
    "test:security": "npm audit --production && npm run test:lock",
    "test:lint": "eslint src bin tests --no-error-on-unmatched-pattern",
    "test:mocha": "mocha --config .mocharc.json 'tests/**/*test.js'",
    "test:package": "npt test -c .package-tester.json",
    "test:coverage": "BUILD=0 MODE=test nyc mocha --config .mocharc.json 'tests/**/*test.js'",
    "test:debt": "jscpd src --config .jscpd.json",
    "test:lock": "lockfile-lint --path package-lock.json --validate-https --allowed-hosts npm",
    "coverage-check": "MODE=test nyc check-coverage",
    "test": "npm run test:lint && npm run test:security && BUILD=1 npm run test:mocha",
    "sort-deps": "npm remove --save example; npm remove --save-dev example",
    "semantic-release": "npx semantic-release"
  },
  "keywords": [
    "csv",
    "logs",
    "winston",
    "formatter",
    "parse",
    "export",
    "Excel",
    "Google Spreadsheet",
    "comma-separated values",
    "reports"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pustovitDmytro/winston-csv-format.git"
  },
  "author": "Dmytro Pustovit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pustovitDmytro/winston-csv-format/issues"
  },
  "engines": {
    "node": ">=10"
  },
  "homepage": "https://github.com/pustovitDmytro/winston-csv-format#readme",
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/node": "^7.22.19",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.23.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.23.8",
    "@babel/runtime": "^7.23.8",
    "@commitlint/cli": "^13.2.1",
    "@commitlint/lint": "^13.2.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "^4.4.1",
    "chance": "^1.1.11",
    "conventional-changelog-eslint": "^3.0.9",
    "coveralls": "^3.1.1",
    "danger": "^10.9.0",
    "eslint": "^7.32.0",
    "eslint-config-incredible": "^2.4.4",
    "eslint-plugin-censor": "^1.5.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-regexp": "^1.15.0",
    "eslint-plugin-scanjs-rules": "^0.2.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-sonarjs": "^0.23.0",
    "eslint-plugin-unicorn": "^36.0.0",
    "fs-extra": "^10.1.0",
    "husky": "^7.0.4",
    "jscpd": "^3.5.10",
    "lockfile-lint": "^4.12.1",
    "mocha": "^9.2.2",
    "mocha-junit-reporter": "^2.2.1",
    "node-package-tester": "^1.3.6",
    "nyc": "^15.1.0",
    "semantic-release": "^19.0.5",
    "semantic-release-telegram": "^1.6.2",
    "winston": "^3.11.0",
    "winston-array-transport": "^1.1.11"
  },
  "peerDependencies": {
    "winston": "^3.3.3"
  }
}
